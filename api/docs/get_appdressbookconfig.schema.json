{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "Configuration",
  "description": "RESTful GET appdressbookconfig route",
  "properties": {
    "companyname": {
      "type": [
        "string",
        "null"
      ],
      "title": "The name of your company",
      "default": null
    },
    "needslogin": {
      "type": [
        "boolean",
        "null"
      ],
      "title": "Use the login to access the API",
      "default": "true"
    },
    "terms": {
      "type": [
        "string",
        "null"
      ],
      "title": "Terms of use page text",
      "default": null
    },
    "privacy": {
      "type": [
        "string",
        "null"
      ],
      "title": "Privacy page text",
      "default": null
    }
  },
  "required": [
    "companyname",
    "needslogin",
    "terms",
    "privacy"
  ]
}